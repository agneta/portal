<md-toolbar>
  <div class="md-toolbar-tools"
    layout="row"
    layout-align="start center"
    layout-margin>
    <md-icon md-svg-src="<%-get_icon('agneta/media')%>"></md-icon>
    <h2 class="title">
      <%-lng('portal.media')%>
    </h2>
  </div>
</md-toolbar>

<md-dialog-content flex="100"
  class="media-explorer"
  layout="column">

  <%-await template('ui/explorer',{
    toolbar: 'media/explorer/toolbar',
    item: 'media/explorer/item'
  })%>
</md-dialog-content>

<%-await template('ui/loader')%>