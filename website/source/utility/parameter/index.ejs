<div ng-if="parameters.length"
  class="parameters ag-margin"
  md-whiteframe="2"
  layout="column">
  <div class="parameter"
    ng-repeat="parameter in parameters"
    ng-if="checkCondition(parameter)">
    <p class="title">{{parameter.title}}</p>
    <%-template('./select')%>
    <%-template('./checkboxes')%>
    <%-template('./radio')%>
    <%-template('./input')%>
  </div>
</div>